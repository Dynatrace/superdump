<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - SuperDump</title>
	@{Html.RenderPartial("_Resources");}
</head>
<body>
	@{Html.RenderPartial("_Nav");}
	<nav class="container body-content" style="float: left !important;">
		@RenderBody()
		<hr />
		<footer>
			<p><a href="https://github.com/Dynatrace/superdump">github.com/Dynatrace/superdump</a></p>
		</footer>
	</nav>

	@RenderSection("scripts", required: false)
</body>
</html>
